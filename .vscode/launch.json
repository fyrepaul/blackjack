  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Personal Playground",
      "program": "${workspaceFolder}/client/custom-test.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "DataStore Unit Test",
      "program": "${workspaceFolder}/node_modules/.bin/jasmine",
      "args": ["spec/data-store/data-store-spec.js"],
      "cwd": "${workspaceRoot}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Dust Helpers",
      "program": "${workspaceFolder}/client/custom-dust-helpers.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Example Program",
      "program": "${workspaceFolder}/../example.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Server",
      "program": "${workspaceFolder}/server/index.js"
    }
  ]
}